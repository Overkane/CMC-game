; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false
resources_spreadsheet_view/fold_docks=true
resources_spreadsheet_view/resource_cell_label_mode=1
resources_spreadsheet_view/freeze_first_column=true

[application]

config/name="CMC-game"
config/version="0.1.0"
config/tags=PackedStringArray("template")
run/main_scene="uid://qvkd8kte8h62"
config/features=PackedStringArray("4.4", "GL Compatibility")
run/max_fps=60
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="uid://ctenb3f0cwdsr"
boot_splash/minimum_display_time=300

[autoload]

GameConstants="*res://autoloads/game_constants.gd"

[debug]

settings/stdout/print_fps=true
gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1
gdscript/warnings/return_value_discarded=1

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[editor]

naming/default_signal_callback_name="_on{NodeName}_{signalName}"
naming/default_signal_callback_to_self_name="_on{SignalName}"

[editor_plugins]

enabled=PackedStringArray("res://addons/resources_spreadsheet_view/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "red",
"res://autoloads/": "orange",
"res://docs/": "gray",
"res://licenses/": "gray",
"res://resources/": "teal",
"res://scenes/": "green",
"res://script_templates/": "gray"
}

[input]

